{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "0.10"
        }
      }
    ]
  ],
  "plugins": [
    [
      "add-module-exports",
      {
        "addDefaultProperty": true
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": true,
        "regenerator": true
      }
    ]
  ]
}
